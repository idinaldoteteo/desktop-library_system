<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="rootStackPane" stylesheets="@../main/DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.adduser.AddUserController">
   <children>
      <AnchorPane fx:id="rootAnchorPane" prefHeight="270.0" prefWidth="500.0">
         <children>
            <VBox AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <JFXTextField fx:id="name" labelFloat="true" promptText="Name" styleClass="promptTextFill_Cadastro" stylesheets="@../main/DarkTheme.css">
                     <VBox.margin>
                        <Insets bottom="13.0" left="13.0" right="13.0" top="20.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <JFXTextField fx:id="userId" labelFloat="true" layoutX="10.0" layoutY="10.0" promptText="User ID" styleClass="promptTextFill_Cadastro" stylesheets="@../main/DarkTheme.css">
                     <VBox.margin>
                        <Insets bottom="13.0" left="13.0" right="13.0" top="13.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <JFXTextField fx:id="mobile" labelFloat="true" layoutX="10.0" layoutY="35.0" promptText="Mobile" styleClass="promptTextFill_Cadastro" stylesheets="@../main/DarkTheme.css">
                     <VBox.margin>
                        <Insets bottom="13.0" left="13.0" right="13.0" top="13.0" />
                     </VBox.margin>
                  </JFXTextField>
                  <JFXTextField fx:id="email" labelFloat="true" layoutX="10.0" layoutY="60.0" promptText="Email" styleClass="promptTextFill_Cadastro" stylesheets="@../main/DarkTheme.css">
                     <VBox.margin>
                        <Insets bottom="13.0" left="13.0" right="13.0" top="13.0" />
                     </VBox.margin>
                  </JFXTextField>
               </children>
            </VBox>
            <HBox alignment="CENTER" layoutX="-159.0" layoutY="257.0" prefHeight="60.0" stylesheets="@../main/DarkTheme.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
               <children>
                  <JFXButton onAction="#BTSave" prefHeight="60.0" prefWidth="150.0" stylesheets="@../main/DarkTheme.css" text="Save">
                     <HBox.margin>
                        <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
                     </HBox.margin>
                  </JFXButton>
                  <JFXButton onAction="#BTCancel" prefHeight="60.0" prefWidth="150.0" stylesheets="@../main/DarkTheme.css" text="Cancel">
                     <HBox.margin>
                        <Insets bottom="10.0" left="15.0" right="15.0" top="10.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</StackPane>
